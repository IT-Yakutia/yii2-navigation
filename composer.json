{
    "name": "it-yakutia/yii2-navigation",
    "description": "Navigation module for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "navigation"],
    "license": "MIT",
    "authors": [
      {
        "name": "Ayaal Kaplin",
        "email": "uraankhayayaal@gmail.com"
      }
    ],
    "require": {
      "yiisoft/yii2": "~2.0.0",
      "uraankhayayaal/yii2-sortable": "*",
      "uraankhayayaal/yii2-materializecomponents": "*",
      "uraankhayayaal/yii2-redactor": "*",
      "creocoder/yii2-nested-sets": "^0.9.0"
    },
    "autoload": {
      "psr-4": {
        "ityakutia\\navigation\\": "src"
      }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ],
    "config": {
      "allow-plugins": {
        "yiisoft/yii2-composer": true
      }
    }
  }